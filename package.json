{
  "name": "audio-player-vue",
  "version": "0.1.0",
  "scripts": {
    "dev": "pnpm run --parallel dev",
    "build": "pnpm run --parallel build",
    "preview": "pnpm run --parallel preview",
    "test": "pnpm run --parallel test",
    "lint": "pnpm run --parallel lint",
    "lint:fix": "pnpm run --parallel lint:fix",
    "typecheck": "pnpm run --parallel typecheck",
    "prepublish": "pnpm --filter @mxwllstn/audio-player-vue run prepublish",
    "release": "pnpm run lint && pnpm run build && pnpm --filter @mxwllstn/audio-player-vue run release",
    "compile": "pnpm --filter @mxwllstn/audio-player-vue run compile",
    "cli": "pnpm --filter @mxwllstn/audio-player-vue run cli"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.2.0",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.32.0",
    "typescript": "^5.9.2",
    "vue": "^3.5.18"
  },
  "resolutions": {
    "@eslint/plugin-kit": ">=0.3.3",
    "form-data": ">=4.0.4"
  }
}
